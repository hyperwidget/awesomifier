{
  "manifest_version": 2,
  "name": "awesomifier",
  "description": "Make the internet awesome again!",
  "version": "1.2",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "exclude_matches": ["*://github.com/*"],
      "js": ["emoji.min.js", "content.js"],
      "run_at": "document_end"
    }
  ]
}
